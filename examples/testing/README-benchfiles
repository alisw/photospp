  generator:	pythia, WeakSingleBoson:ffbar2gmZ
            	tauola, decay mode: 3 (tau => pi nu_tau)
     events:	10M

PHOTOS:
      first:	no
     second:	no
        exp:	yes
     cutoff:	0.01/200

MCTESTER:
   particle:	23 (Z0)
 mass power:	2
 mass scale:	on
 histograms:	120 bins; [0 , 1.1]

   analysis:	ZtautauAnalysis.C
     E_test:	0.01/91.187 (10MeV), max 2 photons

