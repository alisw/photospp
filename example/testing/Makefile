############## Make tests ###########################


all:
	make -C Zee
	make -C Zmumu
	make -C Wenu
	make -C Wmunu
	make -C Htautau


clean:
	rm -f *~
	make -C Zee clean
	make -C Zmumu clean
	make -C Wenu clean
	make -C Wmunu clean
	make -C Htautau clean

clobber: clean
	make -C Zee clobber
	make -C Zmumu clobber
	make -C Wenu clobber
	make -C Wmunu clobber
	make -C Htautau clobber
